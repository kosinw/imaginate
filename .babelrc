{
    "presets": [
       ["@babel/preset-env", {
          "targets": { "browsers": ["last 2 versions"] },
          "corejs": "3",
          "useBuiltIns": "usage"
          }],
        "@babel/react",
        {
          "plugins" : ["@babel/plugin-proposal-class-properties"]
        }
    ],
    "plugins": [
      "babel-plugin-twin",
      "babel-plugin-macros",
      "babel-plugin-styled-components"
    ]
}
